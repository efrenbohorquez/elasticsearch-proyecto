# Configuración de Elasticsearch
# Copia este archivo a .env y completa con tus credenciales

# URL de Elasticsearch Cloud (cloud_id)
ELASTIC_CLOUD_ID=tu_cloud_id_aqui

# API Key de Elasticsearch
ELASTIC_API_KEY=tu_api_key_aqui

# Nombre del índice
INDEX_NAME=index_cuentos

# Configuración de logging
LOG_LEVEL=INFO
